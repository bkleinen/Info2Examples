package info2.staticsearching;

import static org.junit.Assert.*;

import org.junit.Test;

public class SearchElementTest {
	SearchElement<Integer> searcher = new SearchElement<Integer>();

	@Test
	public void testBruteForce() {
		Integer[] a1 = {1,2};
		assertEquals(-1,searcher.bruteForce(0,a1));
		assertEquals(0,searcher.bruteForce(1,a1));
		assertEquals(1,searcher.bruteForce(2,a1));
	}

	@Test
	public void testBinarySearch() {
		Integer[] a1 = {1,2};
		assertEquals(-1,searcher.binarySearch(0,a1));
		assertEquals(0,searcher.binarySearch(1,a1));
		assertEquals(1,searcher.binarySearch(2,a1));
		
	}

}
