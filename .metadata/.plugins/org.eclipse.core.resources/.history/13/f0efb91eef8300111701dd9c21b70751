package info2.staticsearching;

public class SearchElement {
	public void bruteForce(Comparable[] a) {
		int low = 0;
		int high = a.length - 1;
		int mid;
		while (low <= high) {
			mid = (low + high) / 2;
			if (a[mid].compares(x) < 0) {
				low = mid + 1;
			} else if (a[mid].compares(x) > 0) {
				high = mid + 1;
			} else {
				return mid;
			}
		}
		throw new ItemNotFound("BinSearch fails");
	}
}
